{
  "gallery_name": "Food Photography",
  "sources": {
    "directories": [
      "./descriptions"
    ]
  },
  "content_rules": {
    "required_keywords": ["food"],
    "preferred_keywords": ["dish", "plate", "meal", "cuisine", "restaurant", "delicious", "gourmet", "fresh", "ingredients", "cooking"],
    "excluded_keywords": ["landscape", "nature", "building"],
    "min_keyword_matches": 1,
    "case_sensitive": false
  },
  "filters": {
    "min_description_length": 60,
    "preferred_prompts": ["colorful", "detailed"],
    "preferred_models": []
  },
  "output": {
    "max_images": 25,
    "sort_by": "keyword_relevance",
    "include_metadata": true
  }
}
